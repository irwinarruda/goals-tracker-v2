{
  "name": "goals-tracker",
  "version": "0.0.0",
  "main": "./dist/native.js",
  "types": "./dist/native.d.ts",
  "type": "module",
  "exports": {
    "./logic": {
      "import": {
        "types": "./dist/logic.d.ts",
        "default": "./dist/logic.js"
      },
      "require": {
        "types": "./dist/logic.d.cts",
        "default": "./dist/logic.cjs"
      }
    },
    "./native": {
      "import": {
        "types": "./dist/native.d.ts",
        "default": "./dist/native.js"
      },
      "require": {
        "types": "./dist/native.d.cts",
        "default": "./dist/native.cjs"
      }
    },
    "./tokens": {
      "import": {
        "types": "./dist/tokens.d.ts",
        "default": "./dist/tokens.js"
      },
      "require": {
        "types": "./dist/tokens.d.cts",
        "default": "./dist/tokens.cjs"
      }
    },
    "./web": {
      "import": {
        "types": "./dist/web.d.ts",
        "default": "./dist/web.js"
      },
      "require": {
        "types": "./dist/web.d.cts",
        "default": "./dist/web.cjs"
      }
    },
    "./package.json": "./package.json"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist && rm -rf node_modules && rm -rf .turbo",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest --coverage"
  },
  "devDependencies": {
    "@types/react": "18.3.12",
    "@vitest/coverage-v8": "3.1.1",
    "eslint": "^9.21.0",
    "eslint-config-goals": "*",
    "tsup": "^8.0.1",
    "typescript": "5.5.4",
    "typescript-config": "*",
    "vitest": "^3.1.1"
  },
  "dependencies": {
    "@react-native-community/datetimepicker": "^7.2.0",
    "date-fns": "^4.1.0",
    "expo-checkbox": "^4.0.1",
    "expo-image": "^2.0.6",
    "react": "18.3.1",
    "react-native": "0.76.7",
    "react-native-gesture-handler": "~2.20.2",
    "react-native-modal-datetime-picker": "^18.0.0",
    "react-native-reanimated": "3.16.1",
    "react-native-svg": "^15.11.2",
    "uuid": "^11.1.0"
  }
}
