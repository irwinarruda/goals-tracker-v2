{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "react",
    "noEmit": true,
    "module": "Preserve",
    "moduleResolution": "bundler",
    "resolvePackageJsonImports": false,
    "strictNullChecks": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "target": "ESNext"
  }
}
